<script type="text/javascript" >
	//
	if(document.getElementById('filter-term-Winter')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-term-Winter" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-departmentcode-LINGUIST')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-departmentcode-LINGUIST" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-catalognumber-LINGUIST')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-catalognumber-LINGUIST" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-term-Autumn')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-term-Autumn" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-term-Spring')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-term-Spring" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-coursestatus-Active')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-coursestatus-Active" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	if(document.getElementById('filter-catalognumber-LINGUIST')) { }
	else
	{
		var newInput = document.createElement('div');
		newInput.innerHTML = '<input type="hidden" name="filter-catalognumber-LINGUIST" value="on"/>';
		document.getElementById('coursesearch_filter').appendChild(newInput);
	}
	//
	
	var collapsed = '';
	
	showFilter = function(id)
	{
		changeStatus(id, true);
	};

	hideFilter = function(id)
	{
		changeStatus(id, false);
	};

	var animating = false;
	
	changeStatus = function(id, visible)
	{
		if(animating)
			return;
		
		if(visible) {
			animating = true;
			Effect.BlindDown('filter_' + id, {duration:0.3, afterFinish: function(){animating = false;}});
			document.getElementById('filterHeader_' + id).onclick = function() { hideFilter(id); return false; };
		}
		else {
			animating = true;
			Effect.BlindUp('filter_' + id, {duration:0.3, afterFinish: function(){animating = false;}});
			document.getElementById('filterHeader_' + id).onclick = function() { showFilter(id); return false; };
		}
		
		if(collapsed.indexOf(',' + id + ',') != -1)
		{
			collapsed = collapsed.substring(0, collapsed.indexOf(',' + id + ',')) + collapsed.substring(collapsed.indexOf(',' + id + ',') + (',' + id).length);
		}
		else
		{
			if(collapsed.length == 0)
				collapsed = ',';

			collapsed += id;
			collapsed += ',';
		}
	};

	submitFilterForm = function()
	{
		document.getElementById('collapsedFilters').value = collapsed;
		document.getElementById('coursesearch_filter').submit();
	};
	
</script>
		
	</div>
	
	

